    "@remix-run/react": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "@tailwindcss/forms": "^0.5.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.33.0",
    "embed-visualizer": "^1.2.1",
    "express": "^4.17.3",
